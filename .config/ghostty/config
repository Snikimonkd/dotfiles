keybind = clear
# new tab
keybind = ctrl+shift+t=new_tab
keybind = ctrl+shift+n=new_tab
# next/prev tab
keybind = ctrl+shift+h=previous_tab
keybind = ctrl+shift+l=next_tab

# splits
keybind = ctrl+shift+enter=new_split:down
keybind = ctrl+shift+left_bracket=new_split:right

# close tab
keybind = ctrl+shift+w=close_surface
keybind = ctrl+shift+i=close_surface

# swap tabs
keybind = ctrl+shift+comma=move_tab:-1
keybind = ctrl+shift+period=move_tab:1

# base
## keybind = ctrl+h=goto_split:left
keybind = ctrl+shift+j=goto_split:bottom
keybind = ctrl+shift+k=goto_split:top
## keybind = ctrl+l=goto_split:right

keybind = ctrl+shift+m=write_scrollback_file:open

# some standart keybinds
keybind = cmd+c=copy_to_clipboard
keybind = cmd+v=paste_from_clipboard

# font keys
keybind = super+minus=decrease_font_size:1
keybind = super+plus=increase_font_size:1
keybind = super+equal=increase_font_size:1
keybind = super+zero=reset_font_size

